{
  "name": "@uniiem/cv-cipher",
  "type": "module",
  "version": "0.0.0",
  "description": "A simple cipher for the Chinese Core Values",
  "keywords": [
    "core-values",
    "cipher"
  ],
  "repository": {
    "type": "git",
    "url": "git+git@github.com:HoshinoSuzumi/core-values-cipher.git"
  },
  "author": "HoshinoSuzumi <master@uniiem.com>",
  "license": "MIT",
  "main": "dist/index.umd.cjs",
